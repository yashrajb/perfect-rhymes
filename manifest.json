{
	"manifest_version":2,
	"name":"perfect rhymes",
	"version":"2.0",
	"content_scripts":[{

		"matches":[
			"<all_urls>"
		],
		"js":["content.js"]

	}],
	"background":{
		"scripts":["background.js"]
	},
	"browser_action":{
		"default_icon":"image.png",
		"default_popup":"popups/index.html"
	},
	"web_accessible_resources":[
		"images/*.jpg"
	]
}